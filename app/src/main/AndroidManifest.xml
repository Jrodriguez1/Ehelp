<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ehelp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".account.login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".account.SignUp"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name=".account.FindPassword"
            android:label="@string/title_activity_find_password" >
        </activity>
        <activity
            android:name=".account.ResetPassword"
            android:label="@string/title_activity_reset_password" >
        </activity>
        <activity
            android:name=".send.SendHelp"
            android:label="@string/title_activity_send_help" >
        </activity>
    </application>

</manifest>
